<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:CompileBindings="True">
	
    <ControlTheme x:Key="{x:Type ThemeVariantScope}" TargetType="{x:Type ThemeVariantScope}">
        <Setter Property="TextElement.Foreground" Value="{StaticResource Foreground-1-SolidColorBrush}" />
        <Setter Property="TextElement.FontSize" Value="14" />
        <!--<Setter Property="TextElement.FontFamily" Value="{DynamicResource DefaultFontFamily}" />-->
    </ControlTheme>
	
</ResourceDictionary>
