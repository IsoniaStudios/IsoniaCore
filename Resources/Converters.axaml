<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Converters="clr-namespace:IsoniaCore.Resources.Converters"
					x:CompileBindings="True">

	<Converters:MathConverter x:Key="MathConverter" />
	<Converters:BooleanInverterConverter x:Key="BooleanInverterConverter" />
	<Converters:RandomColorConverter x:Key="RandomColorConverter" />
	<Converters:KeyToPathConverter x:Key="KeyToPathConverter" />
	<Converters:VersionConverter x:Key="VersionConverter" />

	<Converters:NullToFalseConverter x:Key="NullToFalseConverter" />
	<Converters:NullToTrueConverter x:Key="NullToTrueConverter" />

	<Converters:TextToUpperConverter x:Key="TextToUpperConverter" />
	<Converters:TextToLowerConverter x:Key="TextToLowerConverter" />

</ResourceDictionary>
